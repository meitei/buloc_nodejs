  <link href="/assets/stylesheets/ui.jqgrid.css" media="all" rel="stylesheet" />
  <link href="/assets/stylesheets/bootstrap.css" media="all" rel="stylesheet" />
  <link href="/assets/stylesheets/base.css" media="all" rel="stylesheet" />
  <script src="/assets/javascripts/lib/require.js" data-main="/assets/javascripts/require.config"></script>
</head>
<body>

<!-- header -->

<div class="container">

  <div class="navbar navbar-fixed-top" style="position: absolute;">
    <div class="navbar-inner">
      <div class="container" style="width: auto; padding: 0 20px;">
        <a class="brand" href="#">Buloc</a>
        <ul class="nav">
          <li><a href="/">Home</a></li>
          <li class="active"><a href="#">AppSetting</a></li>
          <li><a href="#">Link</a></li>
        </ul>
      </div>
    </div>
  </div>
  <br />
  <br />
  <br />

<!--
<ul class="breadcrumb">
  <li><a href="#">Home</a> <span class="divider">/</span></li>
