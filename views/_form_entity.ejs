<form class="well form-horizontal">
<fieldset>

  <div class="control-group">
    <label class="control-label" for="name">name</label>
    <div class="controls">
      <input type="text" id="name" name="name">
      <span class="help-inline"></span>
    </div>
  </div>
  <div class="control-group">
    <label class="control-label" for="caption">caption</label>
    <div class="controls">
      <textarea id="caption" name="caption" rows="2"></textarea>
      <span class="help-inline"></span>
    </div>
  </div>
  <div class="control-group">
    <!-- <label class="control-label" for="cached">cached</label> -->
    <div class="controls">
      <label class="checkbox">
        <input type="checkbox" id="cached" name="cached" value="1"> cached
      </label>
      <span class="help-inline"></span>
    </div>
  </div>
  <div class="control-group">
    <!-- <label class="control-label" for="reflected">reflected</label> -->
    <div class="controls">
      <label class="checkbox">
        <input type="checkbox" id="reflected" name="reflected" value="1"> reflected
      </label>
      <span class="help-inline"></span>
    </div>
  </div>

  <div class="form-actions">
    <a id="dialog_regist" class="btn btn-primary">登録</a>
    <a id="dialog_close" class="btn">閉じる</a>
  </div>

</fieldset>
</form>
